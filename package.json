{
  "name": "PayID",
  "version": "1.0.0",
  "main": "./build/src/index.js",
  "scripts": {
    "copySQL": "copyfiles -u 1 **/*.sql build/src",
    "copyHTML": "copyfiles -u 1 src/html/* build/src",
    "start": "node build/src/index.js",
    "startWatch": "tsc-watch --onSuccess 'npm run start'",
    "test": "nyc mocha 'test/**/*.test.ts'",
    "testWatch": "nyc mocha --watch 'test/**/*.test.ts'",
    "lintNoFix": "eslint . --ext .ts",
    "lint": "eslint . --ext .ts --fix",
    "lintWatch": "chokidar src/**/*.ts -c \"npm run lint\" --initial --verbose",
    "build": "rm -rf build && tsc --project . && npm run copySQL && npm run copyHTML",
    "buildWatch": "tsc --watch --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpring-eng/identity-system.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xpring-eng/identity-system/issues"
  },
  "homepage": "https://github.com/xpring-eng/identity-system#readme",
  "dependencies": {
    "boom": "^7.3.0",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "log4js": "^6.2.1",
    "pg": "8.2.1",
    "prom-client": "^12.0.0",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "@types/boom": "^7.3.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "@types/pg": "^7.14.3",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@xpring-eng/eslint-config-base": "^0.1.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chokidar": "^3.4.0",
    "chokidar-cli": "^2.1.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-tsdoc": "^0.2.4",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.1",
    "tsc-watch": "^4.2.6"
  },
  "description": "",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "all": true,
    "check-leaks": true
  }
}
