{
  "name": "identity-system",
  "version": "1.0.0",
  "main": "./build/src/index.js",
  "scripts": {
    "copySQL": "copyfiles -u 1 **/*.sql build/src",
    "start": "node build/src/index.js",
    "test": "nyc mocha 'test/**/*.test.ts'",
    "lint": "eslint . --ext .ts --fix",
    "lint:watch": "chokidar src/**/*.ts -c \"npm run lint\" --initial --verbose",
    "build": "rm -rf build && tsc --project . && npm run copySQL",
    "buildWatch": "tsc --watch --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpring-eng/identity-system.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xpring-eng/identity-system/issues"
  },
  "homepage": "https://github.com/xpring-eng/identity-system#readme",
  "dependencies": {
    "boom": "^7.3.0",
    "express": "^4.17.1",
    "knex": "^0.20.13",
    "log4js": "^6.2.0",
    "pg": "^8.0.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/boom": "^7.3.0",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/pg": "^7.14.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "chai": "^4.2.0",
    "chokidar": "^3.3.1",
    "chokidar-cli": "^2.1.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2"
  },
  "description": "",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "all": true,
    "check-leaks": true
  }
}
